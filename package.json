    "@types/classnames": "^2.2.10",
    "@types/color": "^3.0.3",
    "@types/cookie": "^0.5.1",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/styled-components": "^5.1.25",
    "classnames": "^2.2.6",
    "color": "^4.2.3",
    "cookie": "^0.5.0",
    "firebase": "^11.0.1",
    "fp-ts": "^2.11.1",
    "io-ts": "^2.2.16",
