    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.25",
    "axios": "^0.27.2",
    "classnames": "^2.2.6",
    "color": "^4.2.3",
    "cookie": "^0.5.0",
    "firebase": "^9.6.10",
