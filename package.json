    "eslint": "^7.5.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vite-plugin-pwa": "^1.0.0",
    "vite-plugin-svgr": "^4.3.0",
    "workbox-window": "^7.3.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
